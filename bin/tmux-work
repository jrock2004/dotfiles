tmux new-window -t "Client Search Organism": -n "Tests" -c ~/Development/organism-clientsearch
tmux new-window -t "Client Search Organism": -n "Git" -c ~/Development/organism-clientsearch
tmux next-window -t "Client Search Organism":
tmux new-session -ds "Common API" -c ~/Development/library-appshell-apis -n "() => {}"
tmux new-window -t "Common API": -n "Tests" -c ~/Development/library-appshell-apis
tmux new-window -t "Common API": -n "Git" -c ~/Development/library-appshell-apis
tmux next-window -t "Common API":
tmux new-session -ds "Component Library" -c ~/Development/library-components-beta -n "() => {}"
tmux new-window -t "Component Library": -n "Tests" -c ~/Development/library-components-beta
tmux new-window -t "Component Library": -n "Git" -c ~/Development/library-components-beta
tmux next-window -t "Component Library":
tmux new-session -ds "Dotfiles" -c ~/.dotfiles -n "() => {}"
tmux next-window -t "Dotfiles":
tmux new-session -ds "E2e Automation" -c ~/Development/tools-e2e-automation -n "() => {}"
tmux new-window -t "E2e Automation": -n "Tests" -c ~/Development/tools-e2e-automation
tmux new-window -t "E2e Automation": -n "Git" -c ~/Development/tools-e2e-automation
tmux next-window -t "E2e Automation":
tmux new-session -ds "Online Booking" -c ~/Development/consumer-online-booking -n "() => {}"
tmux new-window -t "Online Booking": -n "Tests" -c ~/Development/consumer-online-booking
tmux new-window -t "Online Booking": -n "Git" -c ~/Development/consumer-online-booking
tmux next-window -t "Online Booking":
tmux new-session -ds "React Core" -c ~/Development/react-core -n "() => {}"
tmux new-window -t "React Core": -n "Tests" -c ~/Development/react-core
tmux new-window -t "React Core": -n "Git" -c ~/Development/react-core
