#!/usr/bin/env bash

tmux new-session -ds "Appshell Service" -c ~/Development/library-appshell-service -n "() => {}"
tmux new-window -t "Appshell Service": -n "Tests" -c ~/Development/library-appshell-service
tmux new-window -t "Appshell Service": -n "Git" -c ~/Development/library-appshell-service
tmux next-window -t "Appshell Service":
tmux new-session -ds "Calendar" -c ~/Development/calendar -n "() => {}"
tmux new-window -t "Calendar": -n "Tests"
tmux new-window -t "Calendar": -n "Git" -c ~/Development/calendar
tmux next-window -t "Calendar":
tmux new-session -ds "Calendar Widget" -c ~/Development/calendar-widgets -n "() => {}"
tmux new-window -t "Calendar Widget": -n "Tests"
tmux new-window -t "Calendar Widget": -n "Git" -c ~/Development/calendar-widgets
tmux next-window -t "Calendar Widget":
tmux new-session -ds "Class Checkin" -c ~/Development/class-check-in -n "() => {}"
tmux new-window -t "Class Checkin": -n "Tests" -c ~/Development/class-check-in
tmux new-window -t "Class Checkin": -n "Git" -c ~/Development/class-check-in
tmux next-window -t "Class Checkin":
